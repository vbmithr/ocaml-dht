(jbuild_version 1)

(library
 ((name        dht)
  (public_name dht)
  (synopsis "OCaml bindings for Juliusz Chroboczek's dht C library")
  (libraries (bigarray))
  (c_flags (-O2))
  (c_names (monocypher dht_orig dht_stubs))))

